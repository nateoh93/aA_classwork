<!DOCTYPE html>
<html>
  <head>
    <title>Bench</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyBSa8aP02xDOmfhZn38OcQrfrk2gSWWTZg" %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <% if logged_in? %>
      <script type="text/javascript">
        window.currentUser = {
          'id': <%= current_user.id %>,
          'username' : "<%= current_user.username %>"
        }
      </script>
    <% end %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
